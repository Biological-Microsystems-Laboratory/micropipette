setwd("~/Documents/micropipette/data/novice-user-data")
par(mfrow=c(1,4))
novice_user_data <- read.csv(file='novice_user_data.csv', sep=',', header=FALSE)
x200 <- c(200, 200, 200, 200, 200, 200)
plot(x200, novice_user_data$V1, ylim=c(150,280), axes=FALSE, ann=FALSE, cex=2)
points(200,mean(novice_user_data$V1), col="red", pch=3, cex=3)
axis(1, at=200, lab="200 uL", cex.axis=1.4)
Axis(side=2, cex.axis=1.3)
x50 <- c(50, 50, 50, 50, 50, 50)
plot(x50, novice_user_data$V2, ylim=c(30,60), axes=FALSE, ann=FALSE, cex=2)
points(50,mean(novice_user_data$V2), col="red", pch=3, cex=3)
axis(1, at=50, lab="50 uL", cex.axis=1.4)
Axis(side=2, cex.axis=1.3)
x20 <- c(20, 20, 20, 20, 20, 20)
plot(x20, novice_user_data$V3, ylim=c(10,30), axes=FALSE, ann=FALSE, cex=2)
points(20,mean(novice_user_data$V3), col="red", pch=3, cex=3)
axis(1, at=20, lab="20 uL", cex.axis=1.4)
Axis(side=2, cex.axis=1.3)
x10 <- c(10, 10, 10, 10, 10, 10)
plot(x10, novice_user_data$V4, ylim=c(0,15), axes=FALSE, ann=FALSE, cex=2)
points(10,mean(novice_user_data$V4), col="red", pch=3, cex=3)
axis(1, at=10, lab="10 uL", cex.axis=1.4)
Axis(side=2, cex.axis=1.3)
title(main="Novice Users' First Attempt", font.main=1, cex.main=2.7, line=-2, outer=TRUE)
