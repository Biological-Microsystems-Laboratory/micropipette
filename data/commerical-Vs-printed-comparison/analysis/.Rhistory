colnames(commercial_analyzed) <- c("Mean", "Systematic Error","% Sys. err.","Random Error","% Rand. err.")
rownames(commercial_analyzed) <- c("200 uL", "50 uL","20 uL","10 uL")
write.csv(commercial_analyzed, file='commercial_analyzed.csv')
